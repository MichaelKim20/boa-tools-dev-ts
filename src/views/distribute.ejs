<!DOCTYPE html>
<html>
<head>
  <%- include('partials/html_head') %>
</head>
<body>
<div data-role="page">
  <%- include('partials/header') %>
  <div data-role="content" class="jqm-content" style="text-align:center;">
    <h4>Result : <%= result.status %></h4>
    <% if (result.status) { %>
      <h4>Total Count : <%= result.data.total_count %></h4>
      <h4>Send Count : <%= result.data.send_count %></h4>
      <h4>Hash : </h4>
      <% for (var idx = 0; idx < result.data.hash.length; idx++) { %>
        <h4><%= result.data.hash[idx] %></h4>
      <% } %>
    <% } %>
    <% if (!result.status) { %>
      <h4>Error : <%= result.error %></h4>
    <% } %>
    </ul>
  </div>
  <%- include('partials/footer') %>
</div>
</body>
</html>
