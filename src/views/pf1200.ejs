<!DOCTYPE html>
<html>
<head>
  <%- include('partials/html_head') %>
</head>
<body>
<div data-role="page">
  <%- include('partials/header') %>
  <div data-role="content" class="jqm-content" style="text-align:center;">
    <div data-demo-html="true">
      <ul data-role="listview">
        <li>Result : <%= result.status %></li>
      <% if (result.status) { %>
        <li>Total Count : <%= result.data.total_count %></li>
        <li>Send Count : <%= result.data.send_count %></li>
        <li>Hash : </li>
        <% for (var idx = 0; idx < result.data.hash.length; idx++) { %>
          <li><%= ">> " + result.data.hash[idx] %></li>
        <% } %>
      <% } %>
      <% if (!result.status) { %>
        <li>Error : <%= result.error %></li>
      <% } %>
      </ul>
    </div>
    <p></p>
    <a href="/PF1000" data-ajax="false" class="ui-btn ui-btn-b">Back</a>
  </div>
  <%- include('partials/footer') %>
</div>
</body>
</html>
